{
    "routing": {
        "routes": {
            "main/editor": {
                "default:Views.Editor": {
                    "id": "editor",
                    "template": "Editor",
                    "node:OML": {
                        "default .content:FlexCells": {
                            "axis": "x",
                            "min": 10,
                            "children": [
                                "30%",
                                "70%"
                            ],
                            "node:OML": [
                                {
                                    "default:FlexCells": {
                                        "axis": "y",
                                        "min": 10,
                                        "children": [
                                            "30%",
                                            "70%"
                                        ],
                                        "node:OML": [
                                            {
                                                "default:Templates.Books": {
                                                    "css": "books",
                                                    "node:OML": {
                                                        "#books .list:DynamicList": {
                                                            "id": "list",
                                                            "scroll": true,
                                                            "key": "_id",
                                                            "mode": "single",
                                                            "display": {
                                                                "template": "Book",
                                                                "css": "book"
                                                            },
                                                            "list:OSE": "{{json books}}"
                                                        }
                                                    }
                                                }
                                            },
                                            {
                                                "default:Templates.Tree": {
                                                    "css": "tree",
                                                    "node:OML": {
                                                        "#tree .tree:Tree": {
                                                            "id": "tree",
                                                            "key": "_id",
                                                            "show_root": true,
                                                            "toggle": false,
                                                            "scroll": true
                                                        }
                                                    }
                                                }
                                            }
                                        ]
                                    }
                                },
                                {
                                    "default:Views.TextEditor": {
                                        "id": "text_editor",
                                        "template": "TextEditor"
                                    }
                                }
                            ]
                        }
                    }
                },
                "scope": [
                    "public"
                ]
            }
        },
        "options": {
            "history": true,
            "bookmark": true,
            "cache": false,
            "jail": false,
            "eval": false
        },
        "cache": [],
        "errors": {}
    },
    "vapps": {
        "main:Stages.Main": {
            "template": "Stage",
            "use": true,
            "home": "editor",
            "placeholder": "#main",
            "scope": [
                "public"
            ],
            "theater": false
        }
    },
    "preload": {
        "/snd": [],
        "fonts": [],
        "themes/io-globules-ogx/base/fonts": [
            "Roboto-Black.ttf",
            "Roboto-BlackItalic.ttf",
            "Roboto-Bold.ttf",
            "Roboto-BoldItalic.ttf",
            "Roboto-Italic.ttf",
            "Roboto-Light.ttf",
            "Roboto-LightItalic.ttf",
            "Roboto-Medium.ttf",
            "Roboto-MediumItalic.ttf",
            "Roboto-Regular.ttf",
            "Roboto-Thin.ttf",
            "Roboto-ThinItalic.ttf"
        ],
        "/img": [
            "add.svg",
            "book.svg",
            "chapter.svg",
            "remove.svg"
        ],
        "/themes/io-globules-ogx/light/img": [
            "down.svg",
            "left.svg",
            "remove.svg",
            "right.svg",
            "up.svg"
        ],
        "/themes/io-globules-ogx/dark/img": [
            "arrow.svg",
            "down.svg",
            "file.svg",
            "folder_closed.svg",
            "folder_empty.svg",
            "folder_open.svg",
            "left.svg",
            "loading.svg",
            "loop.svg",
            "remove.svg",
            "right.svg",
            "send.svg",
            "trash.svg",
            "up.svg"
        ],
        "/json": [
            "books.json"
        ],
        "/html": [
            "template.Book.html",
            "template.Books.html",
            "template.Editor.html",
            "template.NewBook.html",
            "template.NewChapter.html",
            "template.Stage.html",
            "template.TextEditor.html",
            "template.Tree.html"
        ]
    },
    "popups": {
        "width": 300,
        "height": 400,
        "anim": "fade"
    },
    "windows": {
        "head": "back",
        "width": "100%"
    },
    "views": {},
    "overlays": {},
    "rendertime": false,
    "scope": "public",
    "jwt": false
}